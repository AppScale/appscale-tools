#!/usr/bin/env ruby
# Programmer: Chris Bunch

$VERBOSE = nil # to supress excessive SSL cert warnings


$:.unshift File.join(File.dirname(__FILE__), "..", "lib")
require 'appscale_tools'


if __FILE__ == $0
  options = ParseArgs.get_vals_from_args(ARGV, AppScaleTools::GATHER_LOGS_FLAGS,
    AppScaleTools::GATHER_LOGS_USAGE)
  AppScaleTools.gather_logs(options)
end
