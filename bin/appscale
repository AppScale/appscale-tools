lib = os.path.dirname(__file__) + os.sep + ".." + os.sep + "lib"
sys.path.append(lib)
from appscale import AppScale
from custom_exceptions import UsageException


appscale = AppScale()
if len(sys.argv) < 2:
  appscale.help()

command = sys.argv[1]
if command == "init":
  pass
elif command == "up":
  pass
elif command == "status":
  pass
elif command == "deploy":
  pass
elif command == "destroy":
  pass
else:
  appscale.help()
